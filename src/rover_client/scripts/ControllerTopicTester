#!/usr/bin/python3

import rospy
import math
from rover_main.msg import controllerMap

data_array = []

def convert_x_to_angle(joystick_x):
    print(math.degrees(math.acos(joystick_x)))


def subscriber_callback(data):
    convert_x_to_angle(data.Joystick_RightX)

if __name__ == "__main__":
    rospy.init_node("controller_topic_tester")

    subscriber = rospy.Subscriber("controller_state", data_class=controllerMap, callback=subscriber_callback, queue_size=1000)

    rospy.spin()

